<h1>StaticPages#home</h1>
<p>Find me in app/views/static_pages/home.html.erb</p>

<% if user_signed_in? %>
    <span> Добро пожаловать, <%= current_user.user_profile.second_name %> <%= current_user.user_profile.last_name %> </span>
    <%= button_to 'Выйти', destroy_user_session_path, :method => :delete %>
<% else %>
    <%= button_to 'Зарегистрироваться', new_user_registration_path, :method => :get %>
<% end %>
